CompileFlags:
  Add:
    - -Wno-unused-variable
    - -Wno-infinite-recursion
    - -Wno-array-bounds
    - -Werror=implicit-function-declaration
    - -Wshadow
    - -Wno-shadow-field-in-constructor-modified
    - -Wno-shadow-ivar
    - -Wuninitialized
    - -Wunused-label
    - -Wunused-lambda-capture
  Remove: -Wall
InlayHints:
  Designators: false
  ParameterNames: true
  DeducedTypes: true
  Enabled: true